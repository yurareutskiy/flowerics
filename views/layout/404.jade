block content
  // Content Header (Page header)
  section.content-header
    h1
      | 404 Error Page
    ol.breadcrumb
      li
        a(href='#')
          i.fa.fa-dashboard
          | Home
      li
        a(href='#') Examples
      li.active 404 error
  // Main content
  section.content
    .error-page
      h2.headline.text-info  404
      .error-content
        h3
          i.fa.fa-warning.text-yellow
          | Oops! Page not found.
        p
          | We could not find the page you were looking for.
          | Meanwhile, you may
          a(href='/admin/dashboard') return to dashboard
block javascript
  // jQuery 2.0.2
  script(src='/js/libs/jquery.min.js')
  // Bootstrap
  script(src='/js/bootstrap.min.js', type='text/javascript')
  // AdminLTE App
  script(src='/js/AdminLTE/app.js', type='text/javascript')
  // AdminLTE for demo purposes
  script(src='/js/AdminLTE/demo.js', type='text/javascript')
